<tr class="tr_<%= notification.id %>">
    <td><%= notification.msg %></td>
    <td>
        <div class="btn-group">
            <%= link_to edit_notification_path(notification), remote: true,class: 'btn btn-primary' do %>
            	<i class="icon_pencil"></i>
            <% end %>
            <%= link_to notification_path(notification), method: :delete, remote: true, class: 'btn btn-danger' do %>
            	<i class="icon_close_alt2"></i>
            <% end %>
        </div>
    </td>
</tr>