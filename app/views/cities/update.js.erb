<% if @find_city.present? %>
	toastr.error('Record already present!!!');
<% else %>
	$('.new_city_btn').removeClass("hide");
	$('.panel-body').remove();
	<% if @is_updated %>
		$(".tr_<%= @city.id %>").replaceWith("<%= j render(partial: 'city_data', locals: {city: @city}) %>");
		toastr.success('Record Updated Successfully');
	<% else %>
		toastr.error('Record not updated...');
	<% end %>
<% end %>
onAjaxScroll();
