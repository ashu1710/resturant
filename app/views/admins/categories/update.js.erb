<% if @find_category.present? %>
	toastr.error('Record already present!!!');
<% else %>
	$('.new_category_btn').removeClass("hide");
	$('.panel-body').remove();
	<% if @is_updated %>
		$(".tr_<%= @category.id %>").replaceWith("<%= j render(partial: 'category_data', locals: {category: @category}) %>");
		toastr.success('Record Updated Successfully');
	<% else %>
		toastr.error('Record not updated...');
	<% end %>
<% end %>
onAjaxScroll();
